{
  "name": "vite-kitchen-sink",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/dayjs": "^2.16.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.66",
    "@mui/material": "^5.11.3",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "formik-mui": "^4.0.0-alpha.3",
    "formik-mui-lab": "^1.0.0-alpha.3",
    "msw": "^0.49.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.5",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.12",
    "@storybook/addon-essentials": "^7.0.0-beta.19",
    "@storybook/addon-interactions": "^7.0.0-beta.19",
    "@storybook/react": "^7.0.0-beta.19",
    "@storybook/react-vite": "^7.0.0-beta.19",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/testing-react": "^2.0.0-next.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/css-mediaquery": "^0.1.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.0.1",
    "babel-plugin-formatjs": "^10.3.35",
    "cross-env": "^7.0.3",
    "css-mediaquery": "^0.1.2",
    "eslint": "^8.31.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-formatjs": "^4.3.9",
    "eslint-plugin-storybook": "^0.6.8",
    "jsdom": "^20.0.3",
    "msw-storybook-addon": "^1.6.3",
    "prettier": "^2.8.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "storybook": "^7.0.0-beta.19",
    "storybook-addon-intl": "^2.4.1",
    "storybook-dark-mode": "^2.0.5",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.26.3",
    "whatwg-fetch": "^3.6.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "cross-env CI=true vitest run --coverage",
    "lint": "eslint src .storybook --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "intl:extract": "formatjs extract \"src/**/*.ts*\" --out-file lang/en.json --ignore \"src/**/*.test.tsx\" --ignore \"src/**/*.stories.tsx\" --ignore \"src/vite-env.d.ts\"",
    "intl:compile": "formatjs compile-folder lang src/i18n",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "eslintConfig": {
    "plugins": [
      "formatjs"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "rules": {
      "formatjs/enforce-description": [
        "error",
        "literal"
      ],
      "formatjs/enforce-default-message": [
        "error",
        "literal"
      ],
      "formatjs/enforce-placeholders": "error",
      "formatjs/no-id": "error"
    }
  },
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 6.x"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
