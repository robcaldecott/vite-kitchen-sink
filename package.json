{
  "name": "vite-kitchen-sink",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/dayjs": "^2.11.0",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.63",
    "@mui/material": "^5.2.3",
    "dayjs": "^1.10.7",
    "formik": "^2.2.9",
    "formik-mui": "^4.0.0-alpha.3",
    "formik-mui-lab": "^1.0.0-alpha.3",
    "msw": "^0.36.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.24.2",
    "react-query": "^3.34.0",
    "react-router-dom": "^6.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.7.1",
    "@storybook/addon-essentials": "^6.4.10",
    "@storybook/addon-interactions": "^6.4.10",
    "@storybook/react": "^6.4.10",
    "@storybook/testing-library": "0.0.7",
    "@storybook/testing-react": "^1.2.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/css-mediaquery": "^0.1.1",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.1",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^1.1.3",
    "babel-plugin-formatjs": "^10.3.15",
    "cross-env": "^7.0.3",
    "css-mediaquery": "^0.1.2",
    "eslint": "^8.6.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-formatjs": "^2.20.2",
    "eslint-plugin-storybook": "^0.5.3",
    "jsdom": "^19.0.0",
    "msw-storybook-addon": "^1.5.0",
    "prettier": "^2.5.1",
    "storybook-addon-intl": "^2.4.1",
    "storybook-builder-vite": "^0.1.13",
    "storybook-dark-mode": "^1.0.8",
    "typescript": "^4.4.4",
    "vite": "^2.7.10",
    "vitest": "0.0.139",
    "whatwg-fetch": "^3.6.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "cross-env CI=true vitest run",
    "lint": "eslint src .storybook --max-warnings 0",
    "storybook": "start-storybook -p 6006",
    "intl:extract": "formatjs extract \"src/**/*.ts*\" --out-file lang/en.json --ignore \"src/**/*.test.tsx\" --ignore \"src/**/*.stories.tsx\" --ignore \"src/vite-env.d.ts\"",
    "intl:compile": "formatjs compile-folder lang src/i18n",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "eslintConfig": {
    "plugins": [
      "formatjs"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "rules": {
      "formatjs/enforce-description": [
        "error",
        "literal"
      ],
      "formatjs/enforce-default-message": [
        "error",
        "literal"
      ],
      "formatjs/enforce-placeholders": "error",
      "formatjs/no-id": "error"
    }
  },
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 6.x"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
